@model SistemaIndicadoresAmbientales.Entity.Entrada


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        <div id="Etiqueta">
            <h1 class="H1-Etiqueta">Registrar Entrada de Compost</h1>
        </div>
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div id="Formulario">
            <div class="Formulario">
                <label id="Cantidad" value="Cantidad" name="Cantidad" class="control-label">Cantidad: </label>
                <input type="text" id="Cantidad" name="Cantidad" class="form-control" />

            </div>
            <div class="Formulario">
                <input type="hidden" value="1" id="Id_planta" name="Id_planta" />
            </div>
            <div>
                <div style="text-align: center;">
                    <input value="Ingresar" id="ingresar" name="Registrar" type="submit" class="from-control">
                </div>
            </div>
        </div>
    </div>
}